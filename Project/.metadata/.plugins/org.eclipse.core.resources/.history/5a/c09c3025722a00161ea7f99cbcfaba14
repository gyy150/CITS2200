import java.util.*;
public class Path {
	public LinkedList<Integer> p;
	
	public Path(){
		this.p = new LinkedList<Integer>();
	}
	
	public Path(int s){
		this.p = new LinkedList<Integer>();
		p.add(s);
	}
	public void printPath(){
		for(int i: p ){
			System.out.printf("%5d ", i);
		}
		System.out.println("");
	}
	
}
